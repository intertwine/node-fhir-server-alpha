{
  "name": "@asymmetrik/node-fhir-server-mongo",
  "version": "1.0.4-rc1",
  "description": "Mongo Server implementing @asymmetrik/node-fhir-server-core",
  "main": "src/index.js",
  "repository": "https://github.com/Asymmetrik/node-fhir-server-mongo.git",
  "contributors": [
    "Bryan Young <byoung@asymmetrik.com>",
    "Robert Winterbottom <rwinterbottom@asymmetrik.com>"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=8.6"
  },
  "scripts": {
    "start": "export NODE_ENV=test; node src/index.js;",
    "nodemon": "export NODE_ENV=test; node scripts/nodemon;",
    "test": "export NODE_ENV=test; set -e; npm run test:ci;",
    "test:ci": "npm run test:lint; npm run test:jest;",
    "test:lint": "eslint \"src/**/*.js\"",
    "test:jest": "jest"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "coverageReporters": [
      "text",
      "lcov",
      "json"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/testutils/"
    ]
  },
  "dependencies": {
    "@asymmetrik/node-fhir-server-core": "git+https://github.com/Asymmetrik/node-fhir-server-core.git#develop",
    "jsonwebtoken": "^8.1.0",
    "moment-timezone": "^0.5.14",
    "var": "^0.3.2"
  },
  "devDependencies": {
    "cross-env": "^5.1.6",
    "eslint": "^4.14.0",
    "jest": "^22.4.2",
    "nodemon": "^1.17.2"
  },
  "private": false
}
